{
    "system_definitions": {
        "comment": "The Truth Table - What the apps natively expect. DO NOT EDIT unless patching apps.",
        "actions": {
            "duplicate": {
                "photoshop": "ctrl+j",
                "figma": "ctrl+d"
            },
            "deselect": {
                "photoshop": "ctrl+d",
                "figma": "esc"
            },
            "layer_up": {
                "photoshop": "ctrl+ä",
                "figma": "ctrl+ö"
            },
            "layer_down": {
                "photoshop": "ctrl+#",
                "figma": "ctrl+ä"
            },
            "zoom": {
                "photoshop": "alt+wheel",
                "figma": "ctrl+wheel",
                "type": "gesture"
            },
            "redo": {
                "photoshop": "ctrl+shift+z",
                "figma": "ctrl+y"
            },
            "undo": {
                "photoshop": "ctrl+z",
                "figma": "ctrl+z"
            },
            "group": {
                "photoshop": "ctrl+g",
                "figma": "ctrl+g"
            },
            "ungroup": {
                "photoshop": "ctrl+shift+g",
                "figma": "ctrl+shift+g"
            }
        }
    },
    "profiles": {
        "figma_to_photoshop.json": {
