{
    "profile_name": "Photoshop to Figma",
    "target_apps": [
        "figma"
    ],
    "mappings": [
        {
            "input": "ctrl+j",
            "output": "ctrl+d",
            "description": "Duplicate (PS style in Figma)"
        },
        {
            "input": "ctrl+d",
            "output": "esc",
            "description": "Deselect"
        },
        {
            "input": "ctrl+ä",
            "output": "ctrl+ö",
            "description": "Layer Up"
        },
        {
            "input": "ctrl+#",
            "output": "ctrl+ä",
            "description": "Layer Down"
        },
        {
            "input": "alt+wheel",
            "output": "ctrl+wheel",
            "description": "Zoom"
        },
        {
            "input": "ctrl+shift+Z",
            "output": "ctrl+y",
            "description": "Redo"
        },
        {
            "input": "ctrl+g",
            "output": "ctrl+g",
            "description": "Group (Passthrough)"
        },
        {
            "input": "ctrl+shift+g",
            "output": "ctrl+shift+g",
            "description": "Ungroup (Passthrough)"
        }
    ]
}